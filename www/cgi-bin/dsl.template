<!DOCTYPE HTML>
<html>
<head>
  <title>DSL Stats</title>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.9.3/Chart.bundle.min.js" integrity="sha512-vBmx0N/uQOXznm/Nbkp7h0P1RfLSj0HQrFSzV8m7rOGyj30fYAOKHYvCNez+yM8IrfnW0TCodDEjRqf6fodf/Q==" crossorigin="anonymous"></script>
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.9.3/Chart.min.css" integrity="sha512-/zs32ZEJh+/EO2N1b0PEdoA10JkdC3zJ8L5FTiQu82LR9S/rOQNfQN7U59U9BC12swNeRAz3HSzIL2vpp4fv3w==" crossorigin="anonymous" />

</head>
<body>
  <div>
    <p>Line Uptime:%s</p>
  </div>
  <div class="chart-container" style="position: relative; height:400px; width:90vw">
    <canvas id="FFECSchart"></canvas>
  </div>

  <div class="chart-container" style="position: relative; height:400px; width:90vw">
    <canvas id="FESchart"></canvas>
  </div>

  <script>
var ctx = document.getElementById('FFECSchart');
var myChart = new Chart(ctx, {
            type: 'line',
                        data: {                                                               
                                labels: [%s],                                                 
                                datasets: [{
                                        label: 'ES',
                                        data: [                                               
                                             %s                                               
                                        ],                                                    
                                        fill: false,                                          
                                }]
                        },         
    options: {
        legend: {
            display: false
        },
        title: {
            display: true,
            text: 'DSL Forward Error Corrected Seconds FAR'
        },
        maintainAspectRatio: 0,
        scales: {
            yAxes: [{
                ticks: {
                    Min: 0,
                    Max: 100000
                }
            }]
        }
    }
});

var ctx = document.getElementById('FESchart');
var myChart = new Chart(ctx, {                 
            type: 'line',     
                        data: {                                                     
                                labels: [%s],  
                                datasets: [{ 
                                        label: 'FECS',                              
                                        data: [                                     
                                             %s
                                        ],     
                                        fill: false,
                                }]
                        },                            
    options: {
        legend: {
            display: false
        },
        title: {                                    
            display: true,                          
            text: 'DSL Errored Seconds FAR'
        },                                          
        maintainAspectRatio: 0,                     
        scales: {              
            yAxes: [{                          
                ticks: {                  
                    Min: 0,       
                    Max: 100000  
                }              
            }]                   
        }                  
    }                            
});                              
</script> 

</body>
</html>

